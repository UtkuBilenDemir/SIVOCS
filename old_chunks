  scale_color_manual(values = c("#FC4E07", "#E7B800", "#00AFBB")) + 
  
#  geom_point(color='#377eb8') +
 labs(
    x="Interdisciplinary Involvement",
    y = "Scaleability") 
  ##stat_cor( output.type = "text", method = "spearman") +
  ##stat_regline_equation(output.type = "text")
b<-ggplot(data.df, 
       aes(x = motivation.prob., y = contribToSI.rate.)) + 
  geom_point() + 
  ggtitle("") + 
  geom_smooth(method=lm, se=FALSE, fullrange=TRUE)+
