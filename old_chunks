data.df <- cbind(d1.df,
                 contribToSI.rate.= e1.df_trans)
data.df$domain <- data$domain
a<-ggplot(data.df, 
       aes(x = motivation.welfare., y = contribToSI.rate.)) + 
  geom_point() + 
  ggtitle("") + 
  geom_smooth(method=lm, se=FALSE, fullrange=TRUE)+
