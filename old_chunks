c<-ggplot(data.df, 
       aes(x = motivation.pheno., y = contribToSI.rate.)) + 
  geom_point() + 
  ggtitle("") + 
  geom_smooth(method=lm, se=FALSE, fullrange=TRUE)+
  #geom_jitter(color='#377eb8')+
  geom_jitter()+
  theme_minimal() +
